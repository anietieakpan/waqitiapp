{
  "dashboard": {
    "id": null,
    "title": "Waqiti Compensation System Dashboard",
    "tags": ["waqiti", "compensation", "fintech", "production"],
    "style": "dark",
    "timezone": "UTC",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "resets(waqiti_app_start_time_seconds[1h])",
          "iconColor": "green",
          "titleFormat": "Deployment"
        },
        {
          "name": "Alerts",
          "datasource": "Prometheus", 
          "enable": true,
          "expr": "ALERTS{alertname!=\"Watchdog\"}",
          "iconColor": "red",
          "titleFormat": "{{alertname}}"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(transaction_rollback_initiated_total, environment)",
          "refresh": 1,
          "multi": false,
          "includeAll": false,
          "current": {
            "text": "production",
            "value": "production"
          }
        },
        {
          "name": "provider",
          "type": "query", 
          "datasource": "Prometheus",
          "query": "label_values(external_provider_api_calls_total{environment=\"$environment\"}, provider)",
          "refresh": 1,
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "system_health{environment=\"$environment\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 2}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "UNHEALTHY"},
              {"type": "value", "value": "1", "text": "DEGRADED"},
              {"type": "value", "value": "2", "text": "HEALTHY"}
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "Active Transactions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(transaction_rollback_initiated_total{environment=\"$environment\"}[5m])) * 300",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Success Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(transaction_rollback_completed_total{environment=\"$environment\", successful=\"true\"}[5m])) / sum(rate(transaction_rollback_completed_total{environment=\"$environment\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Avg Response Time",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(transaction_rollback_duration_bucket{environment=\"$environment\"}[5m])) by (le))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Transaction Rollback Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(transaction_rollback_initiated_total{environment=\"$environment\"}[5m])) by (provider)",
            "legendFormat": "Initiated - {{provider}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(transaction_rollback_completed_total{environment=\"$environment\", successful=\"true\"}[5m])) by (provider)",
            "legendFormat": "Completed - {{provider}}",
            "refId": "B"
          },
          {
            "expr": "sum(rate(transaction_rollback_failed_total{environment=\"$environment\"}[5m])) by (provider)",
            "legendFormat": "Failed - {{provider}}",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Transactions/sec",
            "min": 0
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "max": true,
          "min": true
        }
      },
      {
        "id": 6,
        "title": "Response Time Percentiles",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(transaction_rollback_duration_bucket{environment=\"$environment\"}[5m])) by (le))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(transaction_rollback_duration_bucket{environment=\"$environment\"}[5m])) by (le))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(transaction_rollback_duration_bucket{environment=\"$environment\"}[5m])) by (le))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ]
      },
      {
        "id": 7,
        "title": "External Provider Health",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "component_health{component=~\"external_provider_.*\", environment=\"$environment\"}",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "sum(rate(external_provider_api_calls_total{environment=\"$environment\"}[5m])) by (provider)",
            "format": "table",
            "instant": true,
            "refId": "B"
          },
          {
            "expr": "sum(rate(external_provider_api_calls_total{environment=\"$environment\", successful=\"true\"}[5m])) by (provider) / sum(rate(external_provider_api_calls_total{environment=\"$environment\"}[5m])) by (provider) * 100",
            "format": "table",
            "instant": true,
            "refId": "C"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "job": true,
                "instance": true
              },
              "renameByName": {
                "Value #A": "Health Status",
                "Value #B": "Requests/sec",
                "Value #C": "Success Rate %",
                "component": "Component",
                "provider": "Provider"
              }
            }
          }
        ]
      },
      {
        "id": 8,
        "title": "Circuit Breaker States",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "circuit_breaker_state{environment=\"$environment\"}",
            "refId": "A"
          }
        ],
        "heatmap": {
          "yAxis": {
            "unit": "short",
            "decimals": 0
          },
          "colorMode": "spectrum",
          "colorScale": "linear"
        }
      },
      {
        "id": 9,
        "title": "Error Rates by Provider",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(external_provider_api_calls_total{environment=\"$environment\", successful=\"false\", provider=~\"$provider\"}[5m])) by (provider) / sum(rate(external_provider_api_calls_total{environment=\"$environment\", provider=~\"$provider\"}[5m])) by (provider) * 100",
            "legendFormat": "{{provider}} Error Rate",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Error Rate %",
            "min": 0,
            "max": 100
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [5],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 10,
        "title": "SLA Compliance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sla_compliance{environment=\"$environment\"}",
            "legendFormat": "{{sla}} Compliance",
            "refId": "A"
          },
          {
            "expr": "sla_target{environment=\"$environment\"}",
            "legendFormat": "{{sla}} Target",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Compliance Ratio",
            "min": 0.8,
            "max": 1.1
          }
        ],
        "thresholds": [
          {
            "value": 0.99,
            "colorMode": "critical",
            "op": "lt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 11,
        "title": "Database Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(database_operation_duration_bucket{environment=\"$environment\"}[5m])) by (le, operation))",
            "legendFormat": "{{operation}} P95",
            "refId": "A"
          },
          {
            "expr": "database_connection_pool_utilization{environment=\"$environment\"}",
            "legendFormat": "Connection Pool Utilization",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (s) / Utilization",
            "min": 0
          }
        ]
      },
      {
        "id": 12,
        "title": "Queue Metrics", 
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 36},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "queue_depth{environment=\"$environment\"}",
            "legendFormat": "{{queue}} Depth",
            "refId": "A"
          },
          {
            "expr": "queue_processing_rate{environment=\"$environment\"}",
            "legendFormat": "{{queue}} Processing Rate",
            "refId": "B"
          },
          {
            "expr": "queue_error_rate{environment=\"$environment\"}",
            "legendFormat": "{{queue}} Error Rate",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Messages/Rate",
            "min": 0
          }
        ]
      }
    ]
  }
}